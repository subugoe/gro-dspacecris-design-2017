        "active": true
      },
      "fontvalues": {
        "active": true
      },
      "selectornot": {
        "active": true
      }
    },
    "url": "localhost",
    "browsersync": {
      "port": 3020,
      "uiport": 3021
    },
    "log": {
      "active": true,
      "file": "log.log"
    }
  },
  "scripts": {
    "prebuild": "./bin/prebuild.sh",
    "build": "./bin/build.sh",
    "predev": "./bin/predev.sh",
    "dev": "./bin/dev.sh",
    "sassdoc": "./bin/sassdoc.sh"
