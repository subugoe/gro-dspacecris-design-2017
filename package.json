  "config": {
    "img": {
      "webp": {
        "active": true
      },
      "svg": {
        "active": true
      },
      "jpg": {
        "active": true
      },
      "png": {
        "active": true
      }
    },
    "postcss": {
      "calc": {
        "active": true
      },
      "customproperties": {
        "active": true
